package shared.generated.ciel;

import "shared/generated/ciel.protoc";

option java_package = "com.asgow.ciel.protocol.executors";
option java_outer_classname = "Java2";

message Java2TaskPrivate {

	enum TaskType {
		INITIALISE_CLASS = 0;
		RESUME_OBJECT = 1;
	}
	
	required TaskType type = 1;
	
	optional string javaClass = 2;
	optional shared.generated.ciel.Reference javaObject = 3;
	
	repeated shared.generated.ciel.Reference jarRefs = 4;

}